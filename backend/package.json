{
  "name": "mvp2-structured-quote-backend",
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "postinstall": "npx playwright install --with-deps chromium || echo 'playwright optional'",
    "cli": "node ./cli/index.js",
    "inspect": "node ./modules/diagnostics/autoLogInspector.js",
    "daily": "node ./cli/index.js --urls ./cli/seed-urls.txt --snapshot && node ./modules/diagnostics/autoLogInspector.js --since 1d",
    "daily:fast": "node ./cli/index.js --urls ./cli/seed-urls.txt -c 3 -l 30 --snapshot && node ./modules/diagnostics/autoLogInspector.js --since 1d",
    "selfcheck": "node ./scripts/selfCheck.js",
    "snapshot:debug": "node ./scripts/debugSnapshot.js",
"flow": "node ./scripts/testFlowRunner.js"
    "parsehealth": "node ./scripts/parseHealth.js",
    "test": "node --test ./tests --test-reporter=spec"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.0.0",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.15.0",
    "p-limit": "^4.0.0",
    "playwright": "^1.46.0",
    "xlsx": "^0.18.5",
    "node-fetch": "^2.6.7"
  }
}
